doctype html
html(lang="en-US")
  head
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1")
    title My Title
    link(rel="stylesheet" href="vendor/skeleton/css/normalize.css")
    link(rel="stylesheet" href="vendor/skeleton/css/skeleton.css")
    link(rel="stylesheet" href="css/main.css")

  body
    // [if lt IE 9]><script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script><![endif]

    .input-container
      label(for="tickerSymbol") Stock Ticker
      input(type="text").tickerSymbol#tickerSymbol
      label(for="quantity") Quantity
      input(type="text").quantity#quantity
      button.buy-button.
        Buy
    .table-container
      table
        thead
          tr
            th Company Name
            th Price
            th Quantity
            th Day Change
            th Remove
        tbody

    script(src="vendor/jquery/dist/jquery.min.js")
    script(src="vendor/lodash/lodash.min.js")
    script(src="js/main.js")
